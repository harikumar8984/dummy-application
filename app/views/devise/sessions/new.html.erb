<title>Nuryl</title>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="module form-module">
      <div class="toggle"><i class="fa fa-times fa-pencil"></i>

      </div>
      <div class="form">
        <div class="message"><%= flash[:message] %></div>
        <h2>Admin Log in</h2>
        <form>
          <%= f.email_field :email, autofocus: true , placeholder: 'Email'%>
          <%= f.password_field :password, autocomplete: "off", placeholder: 'Password' %>
          <%= f.submit "Log in", class: 'sbt_btn'%>
          <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="mrg-top-5 text-center" style="padding-left: 5px">
              <a href='#' data-toggle='modal' class= 'login-cont-a' data-target='.signup-modal'>SIGN UP</a>
            </div>
          </div>
        </form>
      </div>


    </div>



    <%= render partial: 'registration/new', locals: { user: @user } %>


    </div>
<% end %>



